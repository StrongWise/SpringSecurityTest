<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springsecuritytest.mapper.UserRoleMapper">

    <insert id="insertUserRole" parameterType="com.example.springsecuritytest.model.UserRole">
        -- UserRoleMapper.insertUserRole
        INSERT INTO user_roles (
            user_id
          , role_id
        ) VALUES (
            #{userId}
          , #{roleId}
        )
    </insert>

</mapper>

